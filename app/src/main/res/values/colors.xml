<?xml version="1.0" encoding="utf-8"?>
<resources>
    <color name="color_primary">#6200EE</color>
    <color name="color_primary_dark">#3700B3</color>
    <color name="color_accent">#03DAC5</color>
    <color name="color_white">#ffffff</color>

    <color name="transparent">#0fff</color>

    <color name="color_on_secondary">@color/color_white</color>

    <color name="color_control_light">#39000000</color> <!-- Same as textColorPrimary disabled -->
    <color name="color_black">#000000</color>

    <color name="default_text_color"> #202931</color>
    <color name="color_red">#B00020</color>


    <!-- Scrim colors -->
    <color name="system_ui_scrim_light">#eaeefa</color> <!-- 70% white -->
    <color name="system_ui_scrim_black">#40000000</color> <!-- 25% black -->


    <!-- We use a dark scrim by default since light status is API 23+ -->
    <color name="status_bar_scrim">@color/system_ui_scrim_black</color>
    <!-- We use a dark scrim by default since light nav bar is API 27+ -->
    <color name="nav_bar_scrim">@color/system_ui_scrim_black</color>

    <!-- Android colors-->
    <color name="android_green">#3ddc85</color>
    <color name="android_green_acc">#1FA86D</color>
    <color name="android_green_dark">#258354</color>
    <color name="android_blue">#4285f5</color>
    <color name="android_navy">#073043</color>
    <color name="android_orange">#f86735</color>
    <color name="android_blue_light">#d8effe</color>
    <color name="android_blue_chartreuse">#eff8cf</color>

    <color name="ads_gray">#9e9e9e</color>

    <integer-array name="swipe_refresh">
        <item>@color/android_blue</item>
        <item>@color/android_orange</item>
        <item>@color/ads_gray</item>
        <item>@color/android_green</item>
        <item>@color/android_green_dark</item>
    </integer-array>

</resources>
